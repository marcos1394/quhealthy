<data>
    <!-- Reemplazamos la vista padre `account.report_invoice_document` con TODO unificado, 
         reintroduciendo la columna "Cantidad" y corrigiendo el t-att-class con diccionario -->
    <xpath expr="/t[@t-name='account.report_invoice_document']" position="replace" mode="inner">

        <!-- Estilos para evitar saltos de página -->
        <style>
          .avoid-page-break {
            page-break-inside: avoid !important;
            page-break-before: avoid !important;
            page-break-after: auto !important;
          }
          @media print {
            .avoid-page-break {
              page-break-inside: avoid !important;
            }
          }
          /* Ajuste de fuente y tamaño global */
          * {
            font-size: 10px;
            line-height: 1.2;
          }
        </style>

        <t t-call="web.external_layout">
            <div class="avoid-page-break" style="width: 100%;">
                <!-- Variable `o` con el contexto de idioma -->
                <t t-set="o" t-value="o.with_context(lang=lang)"/>
                <t t-set="forced_vat" t-value="o.fiscal_position_id.foreign_vat"/>

                <!-- ========== ENCABEZADO DE DIRECCIONES ========== -->
                <div class="row">
                    <t t-if="o.partner_shipping_id and (o.partner_shipping_id != o.partner_id)">
                        <!-- Dirección de Envío distinta -->
                        <div class="col-6">
                            <t t-set="information_block">
                                <div groups="account.group_delivery_invoice_address" name="shipping_address_block">
                                    <strong>Dirección de Envío:</strong>
                                    <div t-field="o.partner_shipping_id" t-options="{'widget': 'contact', 'fields': ['address', 'name'], 'no_marker': True}"/>
                                </div>
                            </t>
                        </div>
                        <div class="col-6" name="address_not_same_as_shipping">
                            <t t-set="address">
                                <address class="mb-0" t-field="o.partner_id" t-options="{'widget': 'contact', 'fields': ['address', 'name'], 'no_marker': True}"/>
                                <div t-if="o.partner_id.vat" id="partner_vat_address_not_same_as_shipping">
                                    <t t-if="o.company_id.account_fiscal_country_id.vat_label" t-out="o.company_id.account_fiscal_country_id.vat_label" id="inv_tax_id_label"/>
                                    <t t-else="">RFC</t>:
                                    <span t-field="o.partner_id.vat"/>
                                </div>
                            </t>
                        </div>
                    </t>
                    <t t-elif="o.partner_shipping_id and (o.partner_shipping_id == o.partner_id)">
                        <!-- Dirección de Envío igual a la del cliente -->
                        <div class="offset-col-6 col-6" name="address_same_as_shipping">
                            <t t-set="address">
                                <address class="mb-0" t-field="o.partner_id" t-options="{'widget': 'contact', 'fields': ['address', 'name'], 'no_marker': True}"/>
                                <div t-if="o.partner_id.vat" id="partner_vat_address_same_as_shipping">
                                    <t t-if="o.company_id.account_fiscal_country_id.vat_label" t-out="o.company_id.account_fiscal_country_id.vat_label" id="inv_tax_id_label"/>
                                    <t t-else="">RFC</t>:
                                    <span t-field="o.partner_id.vat"/>
                                </div>
                            </t>
                        </div>
                    </t>
                    <t t-else="">
                        <!-- Caso sin dirección de envío -->
                        <div class="offset-col-6 col-6" name="no_shipping">
                            <t t-set="address">
                                <address class="mb-0" t-field="o.partner_id" t-options="{'widget': 'contact', 'fields': ['address', 'name'], 'no_marker': True}"/>
                                <div t-if="o.partner_id.vat" id="partner_vat_no_shipping">
                                    <t t-if="o.company_id.account_fiscal_country_id.vat_label" t-out="o.company_id.account_fiscal_country_id.vat_label" id="inv_tax_id_label"/>
                                    <t t-else="">RFC</t>:
                                    <span t-field="o.partner_id.vat"/>
                                </div>
                            </t>
                        </div>
                    </t>
                </div>

                <!-- ========== INFORMACIÓN PRINCIPAL DE LA FACTURA ========== -->
                <div class="mt-5 clearfix">
                    <div class="page mb-4">
                        <h2>
                            <!-- Proforma si aplica -->
                            <span t-if="not proforma"/>
                            <span t-else="">PROFORMA</span>

                            <!-- Tipo de documento (Borrador, Cancelada, etc.) -->
                            <span t-if="o.move_type == 'out_invoice' and o.state == 'posted'">Factura</span>
                            <span t-elif="o.move_type == 'out_invoice' and o.state == 'draft'">Borrador de Factura</span>
                            <span t-elif="o.move_type == 'out_invoice' and o.state == 'cancel'">Factura Cancelada</span>
                            <span t-elif="o.move_type == 'out_refund' and o.state == 'posted'">Nota de Crédito</span>
                            <span t-elif="o.move_type == 'out_refund' and o.state == 'draft'">Borrador de Nota de Crédito</span>
                            <span t-elif="o.move_type == 'out_refund' and o.state == 'cancel'">Nota de Crédito Cancelada</span>
                            <span t-elif="o.move_type == 'in_refund'">Nota de Crédito de Proveedor</span>
                            <span t-elif="o.move_type == 'in_invoice'">Factura de Proveedor</span>

                            <!-- Número de Factura (no '/'). -->
                            <span t-if="o.name != '/'" t-field="o.name"/>
                        </h2>
                        <div class="oe_structure"/>

                        <!-- Fechas, Origen, etc. -->
                        <div id="informations" class="row mt-3 mb-1">
                            <div t-attf-class="#{'col-auto col-3 mw-100' if report_type != 'html' else 'col'} mb-2" t-if="o.invoice_date" name="invoice_date">
                                <!-- Etiqueta según tipo -->
                                <t t-if="o.move_type == 'out_invoice'">
                                    <strong>Fecha de Factura:</strong>
                                </t>
                                <t t-elif="o.move_type == 'out_refund'">
                                    <strong>Fecha de Nota de Crédito:</strong>
                                </t>
                                <t t-elif="o.move_type == 'out_receipt'">
                                    <strong>Fecha de Recibo:</strong>
                                </t>
                                <t t-else="">
                                    <strong>Fecha:</strong>
                                </t>
                                <br/>
                                <span t-field="o.invoice_date"/>
                            </div>
                            <div t-attf-class="#{'col-auto col-3 mw-100' if report_type != 'html' else 'col'} mb-2" t-if="o.invoice_date_due and o.move_type == 'out_invoice' and o.state == 'posted'" name="due_date">
                                <strong>Fecha de Vencimiento:</strong><br/>
                                <span t-field="o.invoice_date_due"/>
                            </div>
                            <div t-attf-class="#{'col-auto col-3 mw-100' if report_type != 'html' else 'col'} mb-2" t-if="o.delivery_date" name="delivery_date">
                                <strong>Fecha de Entrega:</strong><br/>
                                <span t-field="o.delivery_date"/>
                            </div>
                            <div t-attf-class="#{'col-auto col-3 mw-100' if report_type != 'html' else 'col'} mb-2" t-if="o.invoice_origin" name="origin">
                                <strong>Origen:</strong><br/>
                                <span t-field="o.invoice_origin"/>
                            </div>
                            <div t-attf-class="#{'col-auto col-3 mw-100' if report_type != 'html' else 'col'} mb-2" t-if="o.partner_id.ref" name="customer_code">
                                <strong>Código de Cliente:</strong><br/>
                                <span t-field="o.partner_id.ref"/>
                            </div>
                            <div t-attf-class="#{'col-auto col-3 mw-100' if report_type != 'html' else 'col'} mb-2" t-if="o.ref" name="reference">
                                <strong>Referencia:</strong><br/>
                                <span t-field="o.ref"/>
                            </div>
                            <div t-attf-class="#{'col-auto col-3 mw-100' if report_type != 'html' else 'col'} mb-2" t-if="o.invoice_incoterm_id" name="incoterm_id">
                                <strong>Incoterm:</strong><br/>
                                <p t-if="o.incoterm_location">
                                    <span t-field="o.invoice_incoterm_id.code"/>
                                    <br/>
                                    <span t-field="o.incoterm_location"/>
                                </p>
                                <span t-else="" t-field="o.invoice_incoterm_id.code" class="m-0"/>
                            </div>
                        </div>

                        <!-- Datos CFDI: Uso, Forma de Pago, etc. -->
                        <t t-set="cfdi_values" t-value="o._l10n_mx_edi_get_extra_invoice_report_values()"/>
                        <t t-set="original_o_grid_layout" t-value="o_grid_layout"/>
                        <t t-set="o_grid_layout" t-value="'col-auto col-3 mw-100 mb-2'"/>

                        <t t-if="cfdi_values.get('usage')">
                            <div name="cfdi_values['usage']" t-att-class="o_grid_layout" style="font-size: 12px;">
                                <strong>Uso:</strong>
                                <p>
                                    <span t-out="cfdi_values['usage']"/>
                                    <span> - </span>
                                    <span t-out="cfdi_values['usage_desc']"/>
                                </p>
                            </div>
                        </t>
                        <t t-if="cfdi_values.get('payment_way')">
                            <div name="l10n_mx_edi_cfdi_payment_way" t-att-class="o_grid_layout" style="font-size: 12px;">
                                <strong>Forma de Pago:</strong>
                                <p t-out="cfdi_values['payment_way']"/>
                            </div>
                        </t>
                        <div name="l10n_mx_edi_cfdi_payment_method" t-att-class="o_grid_layout" style="font-size: 12px;">
                            <strong>Método de Pago:</strong>
                            <p t-out="cfdi_values.get('payment_method')"/>
                        </div>
                        <div name="l10n_mx_edi_cfdi_bank_account" t-if="cfdi_values.get('bank_account')" t-att-class="o_grid_layout">
                            <strong>Cuenta Bancaria:</strong>
                            <p t-out="cfdi_values['bank_account']"/>
                        </div>
                        <t t-set="o_grid_layout" t-value="original_o_grid_layout"/>

                        <!-- Descuentos si hay líneas con discount -->
                        <t t-set="display_discount" t-value="any(l.discount for l in o.invoice_line_ids)"/>
                        <div class="oe_structure"/>

                        <!-- ========== TABLA DE LÍNEAS con columna CANTIDAD ========== -->
                        <table class="table table-sm o_main_table table-borderless" name="invoice_line_table">
                            <thead>
                                <tr>
                                    <th class="text-start" name="th_description">
                                        <span>Descripción</span>
                                    </th>
                                    <!-- Reintroducimos la columna CANTIDAD: name="th_quantity" -->
                                    <th class="text-end" name="th_quantity">
                                        <span>Cantidad</span>
                                    </th>
                                    <th t-attf-class="text-end {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}" name="th_priceunit">
                                        <span>Precio Unitario</span>
                                    </th>
                                    <th name="th_discount" t-if="display_discount" t-attf-class="text-end {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                        <span>Desc.%</span>
                                    </th>
                                    <!-- Columna Unidad (por si otras vistas insertan antes/después) -->
                                    <th class="text-end" name="th_product_uom_unspsc_code">
                                        <span>Unidad</span>
                                    </th>
                                    <th name="th_taxes" t-attf-class="text-start {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                        <span>Impuestos</span>
                                    </th>
                                    <th class="text-end" name="th_subtotal">
                                        <span>Importe</span>
                                    </th>
                                </tr>
                            </thead>
                            <tbody class="invoice_tbody">
                                <t t-set="current_subtotal" t-value="0"/>
                                <t t-set="current_total" t-value="0"/>
                                <t t-set="lines" t-value="o.invoice_line_ids.sorted(                                     key=lambda l: (-l.sequence, l.date, l.move_name, -l.id),                                     reverse=True                                 )"/>

                                <t t-foreach="lines" t-as="line" t-index="line_index" t-last="line_last">
                                    <t t-set="current_subtotal" t-value="current_subtotal + line.price_subtotal"/>
                                    <t t-set="current_total" t-value="current_total + line.price_total"/>

 <tr t-att-class="
(line.display_type == 'line_section' and 'bg-200 fw-bold o_line_section')
 or (line.display_type == 'line_note' and 'fst-italic o_line_note')
 or ''
">
                                        <!-- LÍNEA DE PRODUCTO normal -->
                                        <t t-if="line.display_type == 'product'">
                                            <td name="td_description">
                                                <span t-if="line.name" t-field="line.name" t-options="{'widget': 'text'}"/>
                                            </td>
                                            <!-- CANTIDAD -->
                                            <td name="td_quantity" class="text-end">
                                                <span t-field="line.quantity"/>
                                                <span t-field="line.product_uom_id" groups="uom.group_uom"/>
                                            </td>
                                            <!-- Precio Unitario -->
                                            <td name="td_price_unit" t-attf-class="text-end {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                                <span class="text-nowrap" t-field="line.price_unit"/>
                                            </td>
                                            <!-- Descuento -->
                                            <td name="td_discount" t-if="display_discount" t-attf-class="text-end {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                                <span class="text-nowrap" t-field="line.discount"/>
                                            </td>
                                            <!-- Unidad 
                                            <td name="td_product_uom_unspsc_code" class="text-end">
                                                <span t-out="line.product_uom_id.name"/>
                                            </td> -->
                                            <!-- Impuestos (0% si no hay) -->
<t t-set="taxes" t-value="', '.join([str(tax.invoice_label or tax.name or 'IVA') for tax in line.tax_ids]) or 'IVA (0%)'"/>
                                            <td name="td_taxes" t-attf-class="text-start {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }} {{ 'text-nowrap' if len(taxes) &lt; 10 else '' }}">
                                                <span t-out="taxes"/>
                                            </td>
                                            <!-- Importe -->
                                            <td name="td_subtotal" class="text-end o_price_total">
                                                <span class="text-nowrap" t-field="line.price_subtotal"/>
                                            </td>
                                        </t>

                                        <!-- LÍNEA DE SECCIÓN -->
                                        <t t-elif="line.display_type == 'line_section'">
                                            <td colspan="99">
                                                <span t-field="line.name" t-options="{'widget': 'text'}"/>
                                            </td>
                                            <t t-set="current_section" t-value="line"/>
                                            <t t-set="current_subtotal" t-value="0"/>
                                        </t>

                                        <!-- LÍNEA DE NOTA -->
                                        <t t-elif="line.display_type == 'line_note'">
                                            <td colspan="99">
                                                <span t-field="line.name" t-options="{'widget': 'text'}"/>
                                            </td>
                                        </t>
                                    </tr>

                                    <!-- Subtotal por sección (opcional) -->
                                    <t t-if="current_section and (line_last or lines[line_index+1].display_type == 'line_section')">
                                        <tr class="is-subtotal text-end">
                                            <td colspan="99">
                                                <strong class="mr16">Subtotal</strong>
                                                <span t-out="current_subtotal" t-options="{'widget': 'monetary','display_currency': o.currency_id}"/>
                                            </td>
                                        </tr>
                                    </t>
                                </t>
                            </tbody>
                        </table>

                        <!-- ========== TOTALES (Subtotal + Impuestos + Pagos) ========== -->
                        <div>
                            <div id="right-elements" t-attf-class="#{'col-5' if report_type != 'html' else 'col-12 col-md-5'} ms-5 d-inline-block float-end">
                                <div id="total" class="clearfix row">
                                    <div class="ms-auto">
                                        <table class="table table-sm table-borderless avoid-page-break-inside">
                                            <!-- Subtotal final (o.amount_untaxed) -->
                                            <tr>
                                                <td class="text-end">
                                                    <strong>Subtotal</strong>
                                                </td>
                                                <td class="text-end">
                                                    <span t-field="o.amount_untaxed" t-options="{'widget': 'monetary',                                                                       'display_currency': o.currency_id}"/>
                                                </td>
                                            </tr>

                                            <!-- Impuestos y TOTAL (document_tax_totals) -->
                                            <t t-set="tax_totals" t-value="o.tax_totals or {}"/>
                                            <t t-call="account.document_tax_totals"/>

                                            <!-- Pagos, si aplica -->
                                            <t t-if="print_with_payments">
                                                <t t-if="o.payment_state != 'invoicing_legacy'">
                                                    <t t-set="payments_vals" t-value="o.sudo().invoice_payments_widget and o.sudo().invoice_payments_widget['content'] or []"/>
                                                    <t t-foreach="payments_vals" t-as="payment_vals">
                                                        <tr t-if="payment_vals['is_exchange'] == 0">
                                                            <td>
                                                                <i class="oe_form_field text-end oe_payment_label">
                                                                    Pagado el
                                                                    <t t-out="payment_vals['date']" t-options="{'widget': 'date'}"/>
                                                                </i>
                                                            </td>
                                                            <td class="text-end">
                                                                <span t-out="payment_vals['amount']" t-options="{'widget': 'monetary','display_currency': o.currency_id}"/>
                                                            </td>
                                                        </tr>
                                                    </t>
                                                    <t t-if="len(payments_vals) &gt; 0">
                                                        <tr class="border-black fw-bold">
                                                            <td>Importe Pendiente</td>
                                                            <td class="text-end">
                                                                <span t-field="o.amount_residual"/>
                                                            </td>
                                                        </tr>
                                                    </t>
                                                </t>
                                            </t>
                                        </table>
                                    </div>
                                </div>

                                <!-- Monto en palabras -->
                                <div class="mb-2">
                                    <p class="text-end lh-sm" t-if="o.company_id.display_invoice_amount_total_words">
                                        Monto total en palabras: <br/>
                                        <small class="text-muted lh-sm">
                                            <span t-field="o.amount_total_words"/>
                                        </small>
                                    </p>
                                </div>
                                <t t-call="account.document_tax_totals_company_currency_template"/>

                                <!-- Área de Firma (solo una) -->
                                <div t-if="o.show_signature_area" t-attf-class="#{'col-12' if report_type != 'html' else 'col-5 offset-7 offset-md-0 col-md-12'}">
                                    <t t-if="o.signature">
                                        <img t-if="o.signature" t-att-src="image_data_uri(o.signature)" class="w-100 mx-auto" style="max-height: 6rem;"/>
                                    </t>
                                    <t t-else="">
                                        <!-- Espacio en blanco para firma -->
                                        <div class="col-12" style="min-height: 10rem"/>
                                    </t>
                                    <p class="col-7 mx-auto text-center" style="border-top: thin solid;">
                                        Firma autorizada
                                    </p>
                                </div>
                            </div>

                            <!-- TABLA DE LOTES / SERIES si aplica -->
                            <t groups="stock_account.group_lot_on_invoice">
                                <t t-set="lot_values" t-value="o._get_invoiced_lot_values()"/>
                                <div t-if="not lot_values" class="oe_structure">​</div>
                                <table t-else="" class="table table-sm mt-2" style="width: 50%;" name="invoice_snln_table">
                                    <thead>
                                        <tr>
                                            <th><span>Producto</span></th>
                                            <th class="text-end"><span>Cantidad</span></th>
                                            <th class="text-end"><span>Número de Serie/Lote</span></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr t-foreach="lot_values" t-as="snln_line">
                                            <td>
                                                <t t-esc="snln_line['product_name']"/>
                                            </td>
                                            <td class="text-end">
                                                <t t-esc="snln_line['quantity']"/>
                                                <t t-esc="snln_line['uom_name']" groups="uom.group_uom"/>
                                            </td>
                                            <td>
                                                <t class="text-end" t-esc="snln_line['lot_name']"/>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </t>

                            <!-- SECCIÓN DE TÉRMINOS DE PAGO, QR, COMPLEMENTO CFDI, ETC. -->
                            <div id="payment_term" class="clearfix">
                                <div class="justify-text">
                                    <p t-if="not is_html_empty(o.fiscal_position_id.note)" name="note" class="mb-2">
                                        <span t-field="o.fiscal_position_id.note"/>
                                    </p>
                                </div>
                                <div class="justify-text">
                                    <p t-if="not is_html_empty(o.taxes_legal_notes)" name="taxes_legal_notes" class="mb-2">
                                        <span t-field="o.taxes_legal_notes"/>
                                    </p>
                                </div>
                                <t t-set="payment_term_details" t-value="o.payment_term_details"/>
                                <div class="mb-3">
                                    <span id="payment_terms_note_id" t-if="o.invoice_payment_term_id.note" t-field="o.invoice_payment_term_id.note" name="payment_term"/>
                                    <br/>
                                    <t t-if="o.invoice_payment_term_id.display_on_invoice and payment_term_details">
                                        <div t-if="o.show_payment_term_details" id="total_payment_term_details_table" class="row">
                                            <div t-attf-class="#{'col-10' if report_type != 'html' else 'col-sm-10 col-md-9'}">
                                                <t t-if="o._is_eligible_for_early_payment_discount(o.currency_id,o.invoice_date)">
                                                    <td>
                                                        <span t-options="{'widget': 'monetary',                                                                           'display_currency': o.currency_id}" t-out="o.invoice_payment_term_id._get_amount_due_after_discount(o.amount_total, o.amount_tax)"/>
                                                        pagadero si se liquida antes del
                                                        <span t-out="o.invoice_payment_term_id._get_last_discount_date_formatted(o.invoice_date)"/>
                                                    </td>
                                                </t>
                                                <t t-if="len(payment_term_details) &gt; 1" t-foreach="payment_term_details" t-as="term">
                                                    <div>
                                                        <span t-out="term_index + 1"/>
                                                        - Pago parcial de
                                                        <t t-options="{'widget': 'monetary',                                                                        'display_currency': o.currency_id}" t-out="term.get('amount')" class="text-end"/>
                                                        <span> vence el </span>
                                                        <t t-out="term.get('date')" class="text-start"/>
                                                    </div>
                                                </t>
                                            </div>
                                        </div>
                                    </t>
                                </div>
                                <div class="mb-3" t-if="o.move_type in ('out_invoice', 'in_refund') and o.payment_reference">
                                    <p name="payment_communication">
                                        Referencia de Pago:
                                        <span class="fw-bold" t-field="o.payment_reference"/>
                                        <t t-if="o.partner_bank_id">
                                            <br/>
                                            en esta cuenta:
                                            <span t-field="o.partner_bank_id" class="fw-bold"/>
                                        </t>
                                    </p>
                                </div>

                                <!-- Mostramos QR si hay saldo pendiente -->
                                <t t-set="show_qr" t-value="o.display_qr_code and o.amount_residual &gt; 0"/>
                                <div t-if="not show_qr" name="qr_code_placeholder" class="oe_structure"/>
                                <div id="qrcode" class="d-flex mb-3 avoid-page-break-inside" t-else="">
                                    <div class="qrcode me-3" id="qrcode_image">
                                        <t t-set="qr_code_url" t-value="o._generate_qr_code(silent_errors=True)"/>
